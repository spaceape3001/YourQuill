<!DOCTYPE html>
<HTML>
	<HEAD>
		<META charset="utf-8" />
		<TITLE>{{TITLE}}</TITLE>
		<LINK rel="stylesheet" type="text/css" href="/css" />
    	<SCRIPT src="/js/jquery.js" ></SCRIPT>{{HTML_SCRIPT}}{{HTML_CSS}}
	</HEAD>
	<BODY>
		<TABLE WIDTH="100%">
			<TR>
				<TH ROWSPAN="2"><A HREF="/{{HOME}}"><IMG CLASS="large" SRC="/logo" BORDER="0" ALT="{{ABBREVIATION}}"></A></TH>
				<TH />
				<TH ALIGN="right" VALIGN="top">
				<DIV id="pleaseLogin">
				    <TABLE>
				        <TR>
				            <TD><input type="text" id="user" /></TD>
				            <td><input type="password" id="pass" /></td>
				            <td><button type="button">Login</button></td>
				        </tr>
				    </TABLE>
		        </DIV>
		        <DIV id="weAreLoggedIn">
		            <TABLE>
				        <TR>
				            <TD colspan="2"><B><SPAN id="USERNAME">{{USER}}</SPAN></B></TD>
				            <TD><button type="Logout">Logout</button></TD>
				        </tr>
				    </TABLE>
		        </DIV>
		        <DIV id="weAreLocalUser">
		            <B><SPAN id="LOCAL">Local User: {{LUSER}}</SPAN></B>
		        </DIV>
		        <DIV id="NoJavascript"><B>No Javascript: Read Only Mode</B></DIV>
			</TR>
			<TR>
				<TH COLSPAN="2" ALIGN="center" VALIGN="center"><H1>{{TITLE}}</H1></TH>
			</TR>
			<TR>
				<TD COLSPAN="3"><HR></TD>
			</TR>
			<TR id="tabbar-row">
				<TD COLSPAN="3" WIDTH="100%">{{TABBAR}}</TD>
			</TR>
			<TR id="tabbar-hr">
				<TD COLSPAN="3"><HR></TD>
			</TR>
			<TR>
				<TD COLSPAN="3">{{BODY}}</TD>
			</TR>
			<TR>
				<TD COLSPAN="3"><HR></TD>
			</TR>
			<TR>
				<TD><A HREF="/"><IMG SRC="/img/yquill.svg" HEIGHT="128" BORDER="0" ALT="YQ"></A></TD>
				<TD>{{FOOTER}}</TD>
				<TD ALIGN="right">
					{{USER}}@{{HOST}}:{{PORT}}<BR>
					<i>{{TIME}}</i><br>
					{{SSID}}
				</TD>
			</TR>
		</TABLE>
	    <script>
	        var user          = "{{USER}}";
	        var isLocal       = {{LOCAL}};
	        var canEdit       = {{CAN_EDIT}};
	        var isLoggedIn    = {{LOGGED_IN}};
	        var hasTabbar     = {{HAS_TABBAR}};

            $(document).ready(function(){
                $("#NoJavascript").hide();
                $("#weAreLocalUser").hide();
                if(isLoggedIn){
                    $("#pleaseLogin").hide();
                } else {
                    $("#weAreLoggedIn").hide();                
                }
                if(!hasTabbar){
                    $("#tabbar-hr").hide();
                    $("#tabbar-row").hide();
                }
            });
            {{script}}
	    </script>
        {{SCRIPTS}}
	</BODY>
</HTML>

